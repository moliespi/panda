    {
      "cell_type": "code",
      "source": [
        "#Ejercicio 2:\n",
        "array_cursos=np.array([(\"Matemáticas\"),(\"Física\"),(\"Química\"),(\"Historia\"), (\"Lengua\")])\n",
        "print(\"Yo estudio\",array_cursos[0])\n",
        "print(\"Yo estudio\",array_cursos[1])\n",
        "print(\"Yo estudio\",array_cursos[2])\n",
        "print(\"Yo estudio\",array_cursos[3])\n",
        "print(\"Yo estudio\",array_cursos[4])"
      ],
      "metadata": {
        "colab": {
          "base_uri": "https://localhost:8080/"
        },
        "id": "D4UkIu6kNTDw",
        "outputId": "bbfd93c5-3c7c-4ceb-f2b3-0351db4c2531"
      },
      "execution_count": null,
      "outputs": [
        {
          "output_type": "stream",
          "name": "stdout",
